{
  "type": "array",
  "items": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string"
      },
      "slug": {
        "type": "string",
        "pattern": "^[a-z0-9]+(?:-[a-z0-9]+)*$"
      },
      "flag": {
        "type": "string"
      },
      "description": {
        "type": "string"
      },
      "seoDescription": {
        "type": "string"
      },
      "category": {
        "type": "string",
        "enum": [
          "Invasion & Occupation",
          "Insurgency & Resource Conflict",
          "State Collapse & Civil War",
          "Occupation & Dispossession"
        ]
      }
    },
    "required": [
      "name",
      "slug",
      "flag",
      "description",
      "seoDescription",
      "category"
    ],
    "additionalProperties": false
  },
  "$schema": "http://json-schema.org/draft-07/schema#"
}
